        <activity
            android:name=".Controlador.PreguntasEncuestas.EncuestaDolor"
            android:theme="@style/AppTheme.NoActionBar">
            >
        </activity>
        <activity
            android:name=".Controlador.PreguntasEncuestas.EncuestaNivelDolor"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Controlador.PreguntasEncuestas.EncuestaSintomas"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Controlador.PreguntasEncuestas.EncuestaPartesDolor"
