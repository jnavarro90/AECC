<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.aecc.aecc"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_PROFILE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".Controlador.Login"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme">
            android:windowSoftInputMode="stateVisible|adjustResize">

            <!--
              ATTENTION: This intent was auto-generated. Follow instructions at
              https://g.co/AppIndexing/AndroidStudio to publish your Android app deep links.
            -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <!--
                  ATTENTION: This data URL was auto-generated. We recommend that you use the HTTP scheme.
                  TODO: Change the host or pathPrefix as necessary.
                -->
                <data
                    android:host="aecc.aecc.com"
                    android:pathPrefix="/login"
                    android:scheme="http"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".Controlador.Home"
            android:label="@string/title_activity_home_patient"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Controlador.Calendario"
            android:label="@string/title_activity_calendar"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Controlador.Medication"
            android:label="@string/title_activity_medication"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Controlador.Evolution"
            android:label="@string/title_activity_evolution"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Controlador.Surveys"
            android:label="@string/title_activity_surveys"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Controlador.Profile"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Controlador.Settings"
            android:label="@string/title_activity_settings"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Controlador.PreguntasEncuestas.EncuestaAnimo"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Controlador.PreguntasEncuestas.EncuestaSleep"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Controlador.PreguntasEncuestas.EncuestaActividades"
